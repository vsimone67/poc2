{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.Seq",
            "Serilog.Sinks.Udp"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning"
            }
        },
        "WriteTo": [{
            "Name": "Seq",
            "Args": {
                "serverUrl": "http://192.168.86.182:9400"
            }
        }, {
            "Name": "Console"
        }, {
            "Name": "Udp",
            "Args": {
                "remoteAddress": "192.168.86.153",
                "remotePort": 7071,
                "family": "InterNetwork"
            }
        }],
        "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
        "Properties": {
            "ApplicationName": "MyApp API"
        }
    },
    "Authorization": {
        "Issuer": "XXXX",
        "Audience": "XXXX",
        "TokenExpires": 60
    },
    "ServiceBusSettings": {
        "ServerName": "192.168.86.181",
        "ListenQueueName": "MibComplete",
        "SubmitQueueName": "SubmitMib",
        "NumberOfRetries": "3",
        "RetryInterval": "5000"
    },
    "Management": {
        "Endpoints": {
            "Actuator": {
                "Exposure": {
                    "Include": ["*"]
                }
            },
            "Metrics": {
                "Enabled": true
            }
        },
        "tracing": {
            "Name": "MyApp",
            "alwaysSample": true,
            "useShortTraceIds": false,
            "exporter": {
                "zipkin": {
                    "endpoint": "http://192.168.86.183:9411/api/v2/spans",
                    "validateCertificates": false,
                    "useShortTraceIds ": false,
                    "ServiceName": "MyApp"
                }
            }
        }
    },
    "ReverseProxy": {
        "Routes": [{
            "RouteId": "facroute",
            "ClusterId": "faccluster",
            "Match": {
                "Path": "/fac/{**catch-all}"
            }
        }, 
        {
            "RouteId": "fachealthroute",
            "ClusterId": "faccluster",
            "Match": {
                "Path": "/fac/health"
            },
            "Transforms": [{
                "PathSet": "/actuator/health"
            }]
        }, 
        {
            "RouteId": "facmetricsroute",
            "ClusterId": "faccluster",
            "Match": {
                "Path": "/fac/metrics"
            },
            "Transforms": [{
                "PathSet": "/actuator/metrics"
            }]
        },         
        {
            "RouteId": " ",
            "ClusterId": "faccluster",
            "Match": {
                "Path": "/fac/swagger"
            },
            "Transforms": [{
                "PathSet": "/swagger"
            }]
        }],
        "Clusters": {
            "faccluster": {
                "Destinations": {
                    "cluster1/destination1": {
                        "Address": "http://localhost:4000/"
                    }
                }
            }
        }
    }
}